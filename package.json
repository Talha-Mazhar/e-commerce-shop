{
    "name": "proshop",
    "version": "1.0.0",
    "description": "eCommerce applciation with the MERN stack",
    "type": "module",
    "main": "server.js",
    "scripts": {
        "start": "node backend/server.js",
        "server": "nodemon backend/server.js",
        "client": "npm run dev --prefix frontend",
        "clientCss": "npm run watch --prefix frontend",
        "devs": "concurrently \"npm run server\" \"npm run client\" \"npm run clientCss\""
    },
    "author": "Talha Mazhar",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "mongoose": "^8.3.1"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "dotenv": "^16.4.5",
        "nodemon": "^3.1.0"
    }
}
